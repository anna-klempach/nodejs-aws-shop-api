{
  "name": "nodejs-aws-shop-api",
  "version": "0.1.0",
  "bin": {
    "nodejs-aws-shop-api": "bin/nodejs-aws-shop-api.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "test:dev": "jest --watch",
    "cdk": "cdk",
    "cdk:deploy": "npx cdk deploy --profile HKL",
    "cdk:bootstrap": "npx cdk bootstrap --profile HKL",
    "db:write": "aws --profile HKL dynamodb batch-write-item --request-items file://src/data.json"
  },
  "devDependencies": {
    "@aws-sdk/client-dynamodb": "^3.350.0",
    "@types/aws-lambda": "^8.10.119",
    "@types/jest": "^29.5.1",
    "@types/node": "20.1.7",
    "aws-cdk": "2.81.0",
    "aws-lambda": "^1.0.7",
    "dotenv": "^16.1.4",
    "esbuild": "^0.17.19",
    "jest": "^29.5.0",
    "joi": "^17.9.2",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "~5.0.4",
    "uuid": "^9.0.0"
  },
  "dependencies": {
    "@middy/core": "^4.5.1",
    "aws-cdk-lib": "2.81.0",
    "constructs": "^10.0.0",
    "source-map-support": "^0.5.21"
  }
}
